{
  "name": "clean-start-express",
  "version": "1.0.4",
  "description": "A simple clean webpack 4, babel 7, sass, css, and externalized stylesheet project setup for use",
  "main": "src/index.js",
  "scripts": {
    "dev": "clear && webpack --mode development",
    "build": "clear && webpack --mode production",
    "clean": "rm -rf node_modules",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start": "node ./service/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/clean-start-express.git"
  },
  "keywords": [
    "setup",
    "webpack",
    "babel",
    "mocha",
    "chai",
    "sass",
    "css",
    "express"
  ],
  "authors": [
    "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
    "Karen Calpo <kc@augmentedjs.com> (http://www.augmentedjs.com)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Augmentedjs/clean-start-express/issues"
  },
  "bin": {
    "clean-start-express": "./bin/cse.js"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/Augmentedjs/clean-start-express#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.15.0",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.3",
    "node-sass": "^4.11.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.0",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "fs-extra": "^8.0.1"
  }
}
