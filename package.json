{
  "name": "clean-start-express",
  "version": "1.1.0",
  "description": "A simple clean webpack 4, babel 7, sass, css, and externalized stylesheet project setup for use",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "clean": "rm -rf node_modules",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start": "node ./service/index.js",
    "start:dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/clean-start-express.git"
  },
  "keywords": [
    "setup",
    "webpack",
    "babel",
    "mocha",
    "chai",
    "sass",
    "css",
    "express"
  ],
  "authors": [
    "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
    "Karen Calpo <kc@augmentedjs.com> (http://www.augmentedjs.com)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Augmentedjs/clean-start-express/issues"
  },
  "bin": {
    "clean-start-express": "./bin/cse.js"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/Augmentedjs/clean-start-express#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "whatwg-fetch": "^3.2.0",
    "winston": "^3.3.3"
  }
}
